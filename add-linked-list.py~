
class Node:
    def __init__(self, val:int, next:Node=None):
        self.val = val
        self.next = next
        
        
def add_to_link_list(inp:Node, new_node:int): ->Node:
    nnode = Node(new_node)
    nnode.next = inp
    return nnode

